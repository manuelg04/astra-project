model User {
  id        String   @id @default(cuid())
  email     String   @unique
  password  String
  name      String?
  phone     String?
  avatarUrl String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  brands    Brand[]
  posts     Post[]
  postslikes PostLike[]
  spaceGroupMembers SpaceGroupMember[]
  brandMembers BrandMember[]
  @@index([phone])
  @@index([createdAt])
  @@map("users")
}
